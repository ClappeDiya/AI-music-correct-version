# API Keys and Credentials
STRIPE_PUBLIC_KEY=pk_test_your_stripe_key
STRIPE_SECRET_KEY=sk_test_your_stripe_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# AWS Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-west-2
S3_BUCKET_NAME=your-music-bucket
CLOUDFRONT_DISTRIBUTION_ID=your_distribution_id
CLOUDFRONT_DOMAIN=your.cloudfront.net

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/music_db
REDIS_URL=redis://localhost:6379/0

# Security and Authentication
JWT_SECRET=your_jwt_secret
JWT_EXPIRY=24h
CORS_ORIGINS=http://localhost:3000,https://your-production-domain.com

# Feature Flags
ENABLE_SUBSCRIPTION_MODEL=false
ENABLE_TRACK_BUNDLING=false
ENABLE_DYNAMIC_PRICING=false

# Performance and Caching
CACHE_TTL=3600
MAX_UPLOAD_SIZE=100000000
CDN_ENABLED=true

# Monitoring and Analytics
SENTRY_DSN=your_sentry_dsn
NEW_RELIC_LICENSE_KEY=your_new_relic_key
DATADOG_API_KEY=your_datadog_key

# Email Configuration
SMTP_HOST=smtp.your-provider.com
SMTP_PORT=587
SMTP_USER=your_smtp_user
SMTP_PASSWORD=your_smtp_password
EMAIL_FROM=noreply@your-domain.com

# Third-party Integration
ALGOLIA_APP_ID=your_algolia_app_id
ALGOLIA_API_KEY=your_algolia_api_key
ALGOLIA_SEARCH_KEY=your_algolia_search_key
